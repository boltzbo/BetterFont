{
	"name": "Better Font",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Replace all your Helvetica with better font.",
	"homepage_url": "http://betterfont.com",
	/*
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"default_locale": "en",
	"background": {
		"scripts": [
			"src/background.js"
		],
		"persistent": false
	},
	"options_page": "src/index.html",
	*/
	"web_accessible_resources": [
		"fonts/*.*"
	],
	"content_scripts": [{
			"run_at": "document_start",
			"matches": [
				"<all_urls>"
			],
			"js": [
				"src/inject.js"
			]
		}
	]
}
